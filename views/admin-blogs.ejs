                                    <div class="divider"></div>
                                    <div class="flex items-center justify-between flex-wrap gap10">
                                        <div class="text-tiny">Showing <%= blogs.length %> entries</div>
                                    </div>
                                </div>
                                <!-- Blog Modal (Create/Edit) -->
                                <div class="modal fade" id="blogModal" tabindex="-1" aria-labelledby="blogModalLabel" aria-hidden="true">
                                    <div class="modal-dialog modal-lg modal-dialog-centered">
                                        <div class="modal-content">
                                            <form id="blogForm" enctype="multipart/form-data">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="blogModalLabel">Add Blog</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <input type="hidden" name="blogId" id="blogId">
                                                    <div class="row g-3">
                                                        <div class="col-md-6">
                                                            <label class="form-label">Title</label>
                                                            <input type="text" class="form-control" name="title" id="title" required>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label class="form-label">Author</label>
                                                            <input type="text" class="form-control" name="author" id="author" required>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label class="form-label">Date</label>
                                                            <input type="date" class="form-control" name="date" id="date">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label class="form-label">Category</label>
                                                            <input type="text" class="form-control" name="category" id="category">
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Description</label>
                                                            <textarea class="form-control" name="description" id="description" rows="2"></textarea>
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Meta Title</label>
                                                            <input type="text" class="form-control" name="metatitle" id="metatitle">
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Meta Description</label>
                                                            <textarea class="form-control" name="metadescription" id="metadescription" rows="2"></textarea>
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Highlights Title</label>
                                                            <input type="text" class="form-control" name="highlightsTitle" id="highlightsTitle">
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Highlights Points</label>
                                                            <div class="input-group mb-2">
                                                                <input type="text" class="form-control" id="highlightInput" placeholder="Enter highlight point">
                                                                <button class="btn btn-outline-secondary" type="button" id="addHighlightBtn">Add</button>
                                                            </div>
                                                            <ul class="list-group" id="highlightsList"></ul>
                                                            <input type="hidden" name="highlightsPoints" id="highlightsPoints">
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">More Details</label>
                                                            <textarea class="form-control" name="moredetails" id="moredetails" rows="2"></textarea>
                                                        </div>
                                                        <div class="col-12">
                                                            <label class="form-label">Image</label>
                                                            <input type="file" class="form-control" name="blogImage" id="blogImage" accept="image/*">
                                                            <img id="previewImage" src="" alt="" style="max-width:100px; margin-top:8px; display:none;">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                                                    <button type="submit" class="btn btn-primary" id="saveBlogBtn">Save</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <!-- /Blog Modal -->
                            </div>
                        </div>
                        <style>
                            @media (max-width: 768px) {
                                .table-responsive { overflow-x: auto; }
                                .modal-lg { max-width: 98vw; }
                                .main-content-inner, .main-content-wrap, .wg-box { padding: 8px !important; }
                                .table th, .table td { font-size: 13px; }
                            }
                        </style>
                        <script>
                            // Modal logic
                            let blogModal = null;
                            let highlightsArr = [];

                            // Store blogs in a JS variable for reliable access
                            const blogsData = <%- JSON.stringify(blogs) %>;

                            document.addEventListener('DOMContentLoaded', function() {
                                blogModal = new bootstrap.Modal(document.getElementById('blogModal'));
                                document.getElementById('openBlogModal').addEventListener('click', function() {
                                    resetBlogForm();
                                    blogModal.show();
                                });

                                document.getElementById('addHighlightBtn').addEventListener('click', function() {
                                    const input = document.getElementById('highlightInput');
                                    const val = input.value.trim();
                                    if(val) {
                                        highlightsArr.push(val);
                                        renderHighlights();
                                        input.value = '';
                                    }
                                });

                                document.getElementById('highlightInput').addEventListener('keydown', function(e) {
                                    if(e.key === 'Enter') {
                                        e.preventDefault();
                                        document.getElementById('addHighlightBtn').click();
                                    }
                                });

                                document.getElementById('blogImage').addEventListener('change', function(e) {
                                    const file = e.target.files[0];
                                    if(file) {
                                        const reader = new FileReader();
                                        reader.onload = function(ev) {
                                            const img = document.getElementById('previewImage');
                                            img.src = ev.target.result;
                                            img.style.display = 'block';
                                        }
                                        reader.readAsDataURL(file);
                                    }
                                });

                                document.getElementById('blogForm').addEventListener('submit', async function(e) {
                                    e.preventDefault();
                                    document.getElementById('highlightsPoints').value = JSON.stringify(highlightsArr);

                                    const form = e.target;
                                    const formData = new FormData(form);
                                    const blogId = formData.get('blogId');
                                    let url = '/blog/create';
                                    let method = 'POST';
                                    if(blogId) {
                                        url = '/blogs/edit/' + blogId;
                                        method = 'PUT';
                                    }

                                    try {
                                        const res = await fetch(url, {
                                            method,
                                            body: formData
                                        });
                                        if(res.redirected) {
                                            window.location.href = res.url;
                                            return;
                                        }
                                        if(res.ok) {
                                            location.reload();
                                        } else {
                                            const data = await res.json();
                                            alert(data.message || 'Error saving blog');
                                        }
                                    } catch(err) {
                                        alert('Error saving blog');
                                    }
                                });
                            });

                            function renderHighlights() {
                                const list = document.getElementById('highlightsList');
                                list.innerHTML = '';
                                highlightsArr.forEach((point, idx) => {
                                    const li = document.createElement('li');
                                    li.className = 'list-group-item d-flex justify-content-between align-items-center';
                                    li.textContent = point;
                                    const btn = document.createElement('button');
                                    btn.type = 'button';
                                    btn.className = 'btn btn-sm btn-danger';
                                    btn.textContent = 'Remove';
                                    btn.onclick = function() {
                                        highlightsArr.splice(idx, 1);
                                        renderHighlights();
                                    };
                                    li.appendChild(btn);
                                    list.appendChild(li);
                                });
                            }

                            function resetBlogForm() {
                                document.getElementById('blogForm').reset();
                                document.getElementById('blogId').value = '';
                                document.getElementById('blogModalLabel').textContent = 'Add Blog';
                                highlightsArr = [];
                                renderHighlights();
                                document.getElementById('previewImage').style.display = 'none';
                            }

                            // Edit Blog
                            function openEditBlogModal(blogId) {
                                // Use blogsData for reliable access
                                const blog = blogsData.find(b => b._id === blogId);
                                if(!blog) return;
                                resetBlogForm();
                                document.getElementById('blogModalLabel').textContent = 'Edit Blog';
                                document.getElementById('blogId').value = blog._id || '';
                                document.getElementById('title').value = blog.title || '';
                                document.getElementById('author').value = blog.author || '';
                                // Fix: ensure date is set correctly and field is visible
                                if (blog.date) {
                                    try {
                                        // Accept both string and Date
                                        let d = new Date(blog.date);
                                        if (!isNaN(d.getTime())) {
                                            document.getElementById('date').value = d.toISOString().split('T')[0];
                                        } else {
                                            document.getElementById('date').value = '';
                                        }
                                    } catch (e) {
                                        document.getElementById('date').value = '';
                                    }
                                } else {
                                    document.getElementById('date').value = '';
                                }
                                document.getElementById('category').value = blog.category || '';
                                document.getElementById('description').value = blog.description || '';
                                document.getElementById('metatitle').value = blog.metatitle || '';
                                document.getElementById('metadescription').value = blog.metadescription || '';
                                document.getElementById('highlightsTitle').value = blog.highlightsTitle || '';
                                document.getElementById('moredetails').value = blog.moredetails || '';
                                // Fix: highlightsPoints may be undefined, null, or a stringified array
                                if (Array.isArray(blog.highlightsPoints)) {
                                    highlightsArr = [...blog.highlightsPoints];
                                } else if (typeof blog.highlightsPoints === 'string') {
                                    try {
                                        const arr = JSON.parse(blog.highlightsPoints);
                                        highlightsArr = Array.isArray(arr) ? arr : [];
                                    } catch (e) {
                                        highlightsArr = [];
                                    }
                                } else {
                                    highlightsArr = [];
                                }
                                renderHighlights();
                                // Fix: show preview image if available
                                if(blog.image) {
                                    document.getElementById('previewImage').src = blog.image;
                                    document.getElementById('previewImage').style.display = 'block';
                                } else {
                                    document.getElementById('previewImage').style.display = 'none';
                                }
                                // Ensure all fields are visible (force show in case of modal glitches)
                                [
                                    'title','author','date','category','description','metatitle','metadescription','highlightsTitle','moredetails','blogImage'
                                ].forEach(id => {
                                    const el = document.getElementById(id);
                                    if (el) el.style.display = '';
                                });
                                blogModal.show();
                            }

                            // Delete Blog
                            async function deleteBlog(blogId) {
                                if(!confirm('Are you sure you want to delete this blog?')) return;
                                try {
                                    const res = await fetch('/blogs/delete/' + blogId, { method: 'DELETE' });
                                    const data = await res.json();
                                    if(data.success) {
                                        location.reload();
                                    } else {
                                        alert(data.message || 'Error deleting blog');
                                    }
                                } catch(err) {
                                    alert('Error deleting blog');
                                }
                            }
