                        <!-- main-content-wrap -->
                        <div class="main-content-inner">
                            <div class="main-content-wrap">
                                <div class="flex items-center flex-wrap justify-between gap20 mb-30">
                                    <h3>Blogs Management</h3>
                                    <ul class="breadcrumbs flex items-center flex-wrap justify-start gap10">
                                        <li>
                                            <a href="/dashboard"><div class="text-tiny">Dashboard</div></a>
                                        </li>
                                        <li>
                                            <i class="icon-chevron-right"></i>
                                        </li>
                                        <li>
                                            <a href="#"><div class="text-tiny">Blogs management</div></a>
                                        </li>
                                        <li>
                                            <i class="icon-chevron-right"></i>
                                        </li>
                                        <li>
                                            <div class="text-tiny">All Blogs</div>
                                        </li>
                                    </ul>
                                </div>
                                <!-- Blog Management Responsive Table & Form -->
                                <div class="wg-box">
                                    <div class="flex items-center justify-between gap10 flex-wrap mb-3">
                                        <button class="tf-button style-1 w208" id="openBlogModal"><i class="icon-plus"></i>Add New</button>
                                    </div>
                                    <div class="wg-table table-all-category">
                                        <div class="table-responsive">
                                            <table class="table table-striped align-middle" style="width:100%;">
                                                <thead>
                                                    <tr>
                                                        <th>Image</th>
                                                        <th>Title</th>
                                                        <th>Author</th>
                                                        <th>Date</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% blogs.forEach(blog => { %>
                                                    <tr>
                                                        <td>
                                                            <img src="<%= blog.image %>" alt="" style="width:60px; height:60px; object-fit:cover; border-radius:6px;">
                                                        </td>
                                                        <td><%= blog.title %></td>
                                                        <td><%= blog.author %></td>
                                                        <td><%= blog.date ? new Date(blog.date).toLocaleDateString() : '' %></td>
                                                        <td>
                                                            <button class="btn btn-sm btn-outline-primary me-1" onclick="openEditBlogModal('<%= blog._id %>')"><i class="icon-edit-3"></i></button>
                                                            <button class="btn btn-sm btn-outline-danger" onclick="deleteBlog('<%= blog._id %>')"><i class="icon-trash-2"></i></button>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                                </tbody>
                                            </table>
                                        </div>
