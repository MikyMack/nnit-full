                <!-- Start Course Area -->
                <section class="ed-course section-gap position-relative">
                    <div class="container ed-container">
                        <div class="row">
                            <div class="col-12">
                                <div class="ed-course__filter">
                                    <p class="ed-course__filter-text">
                                        <% 
                                            const startIdx = (pagination.page - 1) * pagination.limit + 1;
                                            const endIdx = Math.min(pagination.page * pagination.limit, pagination.total);
                                        %>
                                        Showing <%= pagination.total === 0 ? 0 : startIdx %>-<%= endIdx %> of <%= pagination.total %> Results
                                    </p>
                                    <div class="ed-hero__search p-0">
                                        <form action="/courses" method="get" class="ed-hero__search-form">
                                            <input 
                                                type="search" 
                                                name="search" 
                                                placeholder="Search your courses"
                                                value="<%= search %>"
                                            />
                                            <% if (currentCategory) { %>
                                                <input type="hidden" name="category" value="<%= currentCategory %>">
                                            <% } %>
                                            <button type="submit">Search<i class="fi-rr-search"></i></button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Category Filters -->
                        <div class="row">
                            <div class="ed-hero__search p-0">
                                <ul class="ed-hero__tags justify-content-center">
                                    <li>
                                        <a 
                                            href="/courses<% if (search) { %>?search=<%= encodeURIComponent(search) %><% } %>" 
                                            class="<%= !currentCategory ? 'active' : '' %>"
                                        >All</a>
                                    </li>
                                    <% courseCategories.forEach(function(cat) { %>
                                        <li>
                                            <a 
                                                href="/courses?category=<%= encodeURIComponent(cat.name) %><% if (search) { %>&search=<%= encodeURIComponent(search) %><% } %>"
                                                class="<%= currentCategory === cat.name ? 'active' : '' %>"
                                            >
                                                <%= cat.name %> (<%= cat.count %>)
                                            </a>
                                        </li>
                                    <% }); %>
                                </ul>
                            </div>
                        </div>

                        <!-- Courses Grid -->
                        <div class="row">
                            <% if (courses && courses.length > 0) { %>
                                <% courses.forEach(function(course) { %>
                                    <div class="col-lg-6 col-xl-4 col-md-6 col-12">
                                        <div class="ed-course__card wow fadeInUp" data-wow-delay=".3s" data-wow-duration="1s">
                                            <a href="/course-details/<%= encodeURIComponent(course.title) %>" class="ed-course__img">
                                                <img src="<%= course.image ? course.image : '/images/course/course-1/1.png' %>" alt="course-img" />
                                            </a>
                                            <% if (course.category) { %>
                                                <a href="/courses?category=<%= encodeURIComponent(course.category) %>" class="ed-course__tag"><%= course.category %></a>
                                            <% } %>
                                            <div class="ed-course__body">
                                                <div class="ed-course__lesson">
                                                    <div class="ed-course__part"><i class="fi-rr-book"></i>
                                                        <p>
                                                            <% 
                                                              
                                                                let curriculumCount = 0;
                                                                if (course.coreModules && Array.isArray(course.coreModules)) {
                                                                    curriculumCount = course.coreModules.length;
                                                                } else if (course.courseInformation && course.courseInformation.curriculum) {
                                                                   
                                                                    curriculumCount = parseInt(course.courseInformation.curriculum) || 0;
                                                                }
                                                            %>
                                                            <%= curriculumCount %> curriculum
                                                        </p>
                                                    </div>
                                                    <div class="ed-course__teacher"><i class="fi fi-rr-graduation-cap"></i>
                                                        <p>
                                                            <% 
                                                               
                                                                let studentCount = '';
                                                                if (course.courseInformation && course.courseInformation.students) {
                                                                    studentCount = course.courseInformation.students;
                                                                }
                                                            %>
                                                            <%= studentCount ? studentCount : '' %> 
                                                            <% if (studentCount) { %>Students<% } %>
                                                        </p>
                                                    </div>
                                                </div>
                                                <a href="/course-details/<%= encodeURIComponent(course.title) %>" class="ed-course__title">
                                                    <h5><%= course.title %></h5>
                                                </a>
                                                <% if (course.description) { %>
                                                    <p class="ed-course__desc"><%= course.description.length > 100 ? course.description.substring(0, 100) + '...' : course.description %></p>
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                <% }); %>
                            <% } else { %>
                                <div class="col-12">
                                    <p class="text-center">No courses found.</p>
                                </div>
                            <% } %>
                        </div>

                        <!-- Pagination -->
                        <div class="row">
                            <div class="col-12">
                                <% if (pagination.totalPages > 1) { %>
                                    <div class="ed-pagination">
                                        <ul class="ed-pagination__list">
                                            <% 
                                                // Show up to 5 pages, with current page in the middle if possible
                                                let startPage = Math.max(1, pagination.page - 2);
                                                let endPage = Math.min(pagination.totalPages, startPage + 4);
                                                if (endPage - startPage < 4) {
                                                    startPage = Math.max(1, endPage - 4);
                                                }
                                            %>
                                            <% if (pagination.page > 1) { %>
                                                <li>
                                                    <a href="/courses?page=<%= pagination.page - 1 %><% if (currentCategory) { %>&category=<%= encodeURIComponent(currentCategory) %><% } %><% if (search) { %>&search=<%= encodeURIComponent(search) %><% } %>">
                                                        <i class="fi-rr-arrow-small-left"></i>
                                                    </a>
                                                </li>
                                            <% } %>
                                            <% for (let i = startPage; i <= endPage; i++) { %>
                                                <li class="<%= i === pagination.page ? 'active' : '' %>">
                                                    <a href="/courses?page=<%= i %><% if (currentCategory) { %>&category=<%= encodeURIComponent(currentCategory) %><% } %><% if (search) { %>&search=<%= encodeURIComponent(search) %><% } %>">
                                                        <%= i.toString().padStart(2, '0') %>
                                                    </a>
                                                </li>
                                            <% } %>
                                            <% if (pagination.page < pagination.totalPages) { %>
                                                <li>
                                                    <a href="/courses?page=<%= pagination.page + 1 %><% if (currentCategory) { %>&category=<%= encodeURIComponent(currentCategory) %><% } %><% if (search) { %>&search=<%= encodeURIComponent(search) %><% } %>">
                                                        <i class="fi-rr-arrow-small-right"></i>
                                                    </a>
                                                </li>
                                            <% } %>
                                        </ul>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </section>

