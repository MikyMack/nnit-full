                <section class="ed-features position-relative">
                    <div class="ed-category__shapes">
                        <img class="ed-category__shape-1 updown-ani" src="/images/features/features-1/shape-1.svg"
                            alt="shape-1" />
                        <img class="ed-category__shape-2 rotate-ani" src="/images/features/features-1/shape-2.svg"
