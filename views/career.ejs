                <!-- Start Course Area -->
                <section class="ed-course ed-course--style2 section-gap position-relative overflow-hidden">
                    <div class="container ed-container">
                        <div class="row justify-content-center">
                            <div class="col-lg-6 col-md-8 col-12">
                                <div class="ed-section-head text-center">
                                    <span class="ed-section-head__sm-title">JOB OPPORTUNITIES</span>
                                    <h3 class="ed-section-head__title ed-split-text left">
                                        Find Your Dream Job Today
                                    </h3>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <% if (jobs && jobs.length) { %>
                                <% jobs.forEach(function(job) { %>
                                    <div class="col-lg-6 col-xl-6 col-md-6 col-12">
                                        <div class="ed-course__card ed-course__card--style2 wow fadeInUp" data-wow-delay=".3s"
                                            data-wow-duration="1s">
                                            <div class="ed-course__head position-relative">
                                                <a href="#0" class="ed-course__img">
                                                    <img src="/images/about/about-1/about-img.png" alt="company-logo" />
                                                </a>
                                                <a href="#" class="ed-course__tag"><%= job.category %></a>
                                            </div>
                                            <div class="ed-course__body">
                                                <a href="#0" class="ed-course__title">
                                                    <h5><%= job.title %></h5>
                                                </a>
                                                <!-- Make all icon+content rows flex row-aligned -->
                                                <div class="ed-course__lesson">
                                                    <div class="ed-course__part d-flex align-items-center mb-2">
                                                        <i class="fi fi-rr-briefcase me-2"></i>
                                                        <p class="mb-0"><%= job.employmentType %></p>
                                                    </div>
                                                    <div class="ed-course__teacher d-flex align-items-center mb-2">
                                                        <i class="fi fi-rr-user me-2"></i>
                                                        <p class="mb-0">Experience: <%= job.experience %></p>
                                                    </div>
                                                </div>
                                                <div class="ed-course__bottom d-flex flex-column justify-content-start align-items-start gap-1">
                                                    <% if (job.salaryRange) { %>
                                                        <span class="ed-course__price"><%= job.salaryRange %></span>
                                                    <% } %>
                                                    <div class="ed-course__students d-flex align-items-center text-start mb-2">
                                                        <i class="fi fi-rr-map-marker me-2"></i>
                                                        <p class="job-location mb-0"><%= job.location %></p>
                                                    </div>
                                                    <div class="d-flex align-items-center text-start mb-2">
                                                        <i class="fi fi-rr-calendar me-2"></i>
                                                        <p class="job-posted mb-0">
                                                            Posted: 
                                                            <%= job.postedDate 
                                                                ? new Date(job.postedDate).toLocaleDateString('en-US', { day: '2-digit', month: 'short', year: 'numeric' }) 
                                                                : new Date(job.createdAt).toLocaleDateString('en-US', { day: '2-digit', month: 'short', year: 'numeric' }) 
                                                            %>
                                                        </p>
