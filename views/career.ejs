                                            <% 
                                                let page = pagination.page;
                                                let totalPages = pagination.totalPages;
                                                let search = typeof search !== 'undefined' ? search : '';
                                                let currentCategory = typeof currentCategory !== 'undefined' ? currentCategory : '';
                                                function buildPageUrl(p) {
                                                    let params = [];
                                                    if (search) params.push('search=' + encodeURIComponent(search));
                                                    if (currentCategory) params.push('category=' + encodeURIComponent(currentCategory));
                                                    params.push('page=' + p);
                                                    return '/career?' + params.join('&');
                                                }
                                            %>
                                            <% if (page > 1) { %>
                                                <li>
                                                    <a href="<%= buildPageUrl(page - 1) %>"><i class="fi-rr-arrow-small-left"></i></a>
                                                </li>
                                            <% } %>
                                            <% for (let i = 1; i <= totalPages; i++) { %>
                                                <li class="<%= i === page ? 'active' : '' %>">
                                                    <a href="<%= buildPageUrl(i) %>">
                                                        <%= i.toString().padStart(2, '0') %>
                                                    </a>
                                                </li>
                                            <% } %>
                                            <% if (page < totalPages) { %>
                                                <li>
                                                    <a href="<%= buildPageUrl(page + 1) %>"><i class="fi-rr-arrow-small-right"></i></a>
                                                </li>
