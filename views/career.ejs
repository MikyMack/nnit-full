                            </div>
                            <!-- Single Slider -->
                            <div class="ed-hero__slider-item">
                                <img src="/images/hero/home-3/hero-img-2.jpg" alt="hero-img-2" />
                            </div>
                            <!-- Single Slider -->
                            <div class="ed-hero__slider-item">
                                <img src="/images/hero/home-3/hero-img-3.jpg" alt="hero-img-3" />
                            </div>
                        </div>

                        <div class="container ed-container-expand">
                            <div class="row justify-content-center">
                                <div class="col-lg-7 col-12">
                                    <!-- Hero Content -->
                                    <div class="ed-hero__content text-center">
                                        <h1 class="ed-hero__content-title text-white ed-split-text left">
                                            Find Your
                                            Dream Job Today
                                        </h1>
                                        <p class="text-white ">
                                            Explore thousands of opportunities, connect with top employers, and land
                                            your ideal job with ease.
                                        </p>
                                        <div class="ed-hero__search p-0">
                                            <form action="/career" method="get" class="ed-hero__search-form">
                                                <input type="search" name="search"
                                                    placeholder="Search for jobs, companies or skills"
                                                    value="<%= typeof search !== 'undefined' ? search : '' %>" />
                                                <button type="submit">Search<i class="fi-rr-search"></i></button>
                                            </form>
                                            <ul class="ed-hero__tags justify-content-center">
                                                <% if (jobCategories && jobCategories.length) { %>
                                                    <% jobCategories.slice(0, 5).forEach(function(category) { %>
                                                        <li>
                                                            <a href="/career?<%= search ? 'search=' + encodeURIComponent(search) + '&' : '' %>category=<%= encodeURIComponent(category) %>"
                                                                <%= currentCategory === category ? 'class="active"' : '' %>>
                                                                <%= category %>
                                                            </a>
                                                        </li>
                                                    <% }); %>
                                                <% } else { %>
                                                    <li><a href="#">IT & Software</a></li>
                                                    <li><a href="#">Marketing</a></li>
                                                    <li><a href="#">Design & Creative</a></li>
                                                    <li><a href="#">Finance</a></li>
                                                    <li><a href="#">Healthcare</a></li>
                                                <% } %>
