                <div id="guestDetailsPopupOverlay" class="guest-popup-overlay" style="display:none;">
                    <div class="guest-popup-form-container">
                        <form id="guestDetailsPopupForm" class="guest-popup-form" method="post" action="/start" autocomplete="off">
                            <div class="guest-popup-header">
                                <span class="guest-popup-title">
                                    <i class="fi fi-rr-user"></i> Enter Your Details to Start Assessment
                                </span>
                                <button type="button" class="guest-popup-close" id="guestPopupCloseBtn" aria-label="Close">&times;</button>
