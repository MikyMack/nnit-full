                    doc.setFontSize(14);
                    doc.text('Assessment Report', 14, 16);
                    doc.setFontSize(11);
                    doc.text('Name: ' + (document.getElementById('name')?.value || ''), 14, 26);
                    doc.text('Email: ' + (document.getElementById('email')?.value || ''), 14, 33);
                    doc.text('Mode: ' + mode.toUpperCase(), 14, 40);
                    doc.text('Generated on: ' + new Date().toLocaleString(), 14, 47);
                    doc.save('Assessment_Report.pdf');
                }
            }
            // Setup download button handlers
            function setupDownloadHandlers(){
                var downloadPdfBtn = document.getElementById('downloadPdfBtn');
                var downloadFullReportBtn = document.getElementById('downloadFullReportBtn');
                
                if (downloadPdfBtn){
                    downloadPdfBtn.onclick = function(){
                        if (!isLoggedIn()){
                            pendingDownload = true;
                            var loginEl = document.getElementById('loginModal');
                            if (loginEl){
                                var lm = new bootstrap.Modal(loginEl);
                                lm.show();
                            }
                            return;
                        }
                        performDownload();
                    };
                }
                
                if (downloadFullReportBtn){
                    downloadFullReportBtn.onclick = function(){
                        performFullReportDownload();
                    };
                }
            }
            // Intercept login form submit to set login flag and then download if pending
            var loginForm = document.querySelector('#loginModal form');
            if (loginForm){
                loginForm.addEventListener('submit', function(ev){
                    ev.preventDefault();
                    try { localStorage.setItem('iimf_logged_in','true'); } catch(e){}
                    var loginEl = document.getElementById('loginModal');
                    if (loginEl){
                        var modalInst = bootstrap.Modal.getInstance(loginEl) || new bootstrap.Modal(loginEl);
                        modalInst.hide();
                    }
                    if (pendingDownload){ pendingDownload = false; performDownload(); }
                });
            }
        })();
        </script>
    </body>


</html>
