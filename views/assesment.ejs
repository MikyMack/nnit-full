                                <input type="hidden" name="assessmentId" id="guestPopupAssessmentId" />
                                <div class="mb-3">
                                    <label for="guestPopupName" class="form-label">Name</label>
                                    <input type="text" class="form-control" id="guestPopupName" name="name" required autocomplete="off">
                                </div>
                                <div class="mb-3">
                                    <label for="guestPopupEmail" class="form-label">Email</label>
                                    <input type="email" class="form-control" id="guestPopupEmail" name="email" required autocomplete="off">
                                </div>
                                <div class="mb-3">
                                    <label for="guestPopupPhone" class="form-label">Phone</label>
                                    <input type="tel" class="form-control" id="guestPopupPhone" name="phone" required pattern="[0-9]{10,}" autocomplete="off">
                                </div>
                            </div>
                            <div class="guest-popup-footer">
                                <button type="submit" class="btn btn-primary w-100" id="guestPopupSubmitBtn">
                                    <i class="fi fi-rr-play"></i> Start Assessment
                                </button>
                            </div>
                        </form>
                    </div>
