                <script>
                    document.addEventListener('DOMContentLoaded', function() {
                        var guestBtns = document.querySelectorAll('.start-guest-btn');
                        var popupOverlay = document.getElementById('guestDetailsPopupOverlay');
                        var popupForm = document.getElementById('guestDetailsPopupForm');
                        var popupCloseBtn = document.getElementById('guestPopupCloseBtn');
                        var popupSubmitBtn = document.getElementById('guestPopupSubmitBtn');
                
                        guestBtns.forEach(function(btn) {
                            btn.addEventListener('click', function() {
                                var assessmentId = btn.getAttribute('data-assessment-id');
                                var assessmentTitle = btn.getAttribute('data-assessment-title');
                                console.log('Opening guest popup for assessment:', assessmentId, assessmentTitle);
                                document.getElementById('guestPopupAssessmentId').value = assessmentId;
                                popupOverlay.style.display = 'flex';
                                setTimeout(function() {
                                    document.getElementById('guestPopupName').focus();
                                }, 200);
                            });
                        });
                
                        popupCloseBtn.addEventListener('click', function() {
                            popupOverlay.style.display = 'none';
                            popupForm.reset();
                            resetSubmitButton();
                        });
                
                        // Close popup if clicking outside the form
                        popupOverlay.addEventListener('click', function(e) {
                            if (e.target === popupOverlay) {
                                popupOverlay.style.display = 'none';
                                popupForm.reset();
                                resetSubmitButton();
                            }
                        });
                
                        function resetSubmitButton() {
                            popupSubmitBtn.innerHTML = '<i class="fi fi-rr-play"></i> Start Assessment';
                            popupSubmitBtn.disabled = false;
                        }
                
                        popupForm.addEventListener('submit', function(e) {
                            e.preventDefault();
                
                            const assessmentId = document.getElementById('guestPopupAssessmentId').value;
                            const name = document.getElementById('guestPopupName').value;
                            const email = document.getElementById('guestPopupEmail').value;
                            const phone = document.getElementById('guestPopupPhone').value;
                
                            console.log('Submitting guest form:', { assessmentId, name, email, phone });
                
                            // Validate form
                            if (!name || !email || !phone) {
                                alert('Please fill all required fields');
                                return;
                            }
                
                            popupSubmitBtn.innerHTML = '<i class="fi fi-rr-spinner fi-spin me-2"></i> Starting...';
                            popupSubmitBtn.disabled = true;
                
                            // Test if the endpoint exists first
                            fetch('/start', {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json',
                                },
                                body: JSON.stringify({
                                    assessmentId: assessmentId,
                                    name: name,
                                    email: email,
                                    phone: phone
                                })
                            })
                            .then(response => {
                                console.log('Response status:', response.status);
                                console.log('Response headers:', response.headers);
                                
                                if (!response.ok) {
                                    throw new Error(`HTTP error! status: ${response.status}`);
                                }
                                return response.json();
                            })
                            .then(data => {
                                console.log('Response data:', data);
                                
                                if (data.success) {
                                    console.log('Redirecting to:', '/take-assessment/' + data.attempt._id);
                                    window.location.href = '/take-assessment/' + data.attempt._id;
                                } else {
                                    alert('Error: ' + (data.message || 'Unknown error'));
                                    resetSubmitButton();
                                }
                            })
                            .catch(error => {
                                console.error('Fetch error details:', error);
                                console.error('Error name:', error.name);
                                console.error('Error message:', error.message);
                                
                                // More specific error messages
                                if (error.name === 'TypeError') {
                                    alert('Network error: Cannot connect to server. Please check your internet connection.');
                                } else if (error.message.includes('404')) {
                                    alert('Error: The start endpoint was not found (404). Please contact support.');
                                } else if (error.message.includes('500')) {
                                    alert('Error: Server error. Please try again later.');
                                } else {
                                    alert('Network error: ' + error.message);
                                }
                                resetSubmitButton();
                            });
                        });
                    });
                </script>
