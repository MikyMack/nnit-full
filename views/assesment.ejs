            function generateLikertResults(){
                var dimToScore = {};
                var dimToCount = {};
                
                questions.forEach(function(q, i){
                    var valIdx = typeof answers[i] === 'number' ? answers[i] : -1;
                    var points = valIdx >= 0 ? (4 - valIdx) : 0; // MA=4, SA=3, SD=2, MD=1
                    dimToScore[q.dim] = (dimToScore[q.dim] || 0) + points;
                    dimToCount[q.dim] = (dimToCount[q.dim] || 0) + 1;
                });

                var intelligenceAreas = [
                    'Linguistic', 'Logicalâ€“Mathematical', 'Spatialâ€“Visual', 'Musicalâ€“Rhythmic',
                    'Bodilyâ€“Kinesthetic', 'Interpersonal', 'Intrapersonal', 'Naturalistic'
                ];

                var intelligenceData = {};
                intelligenceAreas.forEach(function(area){
                    var score = dimToScore[area] || 0;
                    var count = dimToCount[area] || 1;
                    intelligenceData[area] = Math.round((score / (count * 4)) * 100);
                });

                assessmentResults = {
                    mode: 'likert',
                    intelligenceData: intelligenceData,
                    dominantIntelligences: getDominantIntelligences(intelligenceData),
                    overallScore: Math.round(Object.values(intelligenceData).reduce((a,b) => a+b, 0) / Object.keys(intelligenceData).length),
                    careerStreams: getCareerStreams(intelligenceData)
                };
            }

            function generateMCQResults(){
                var correct = 0;
                questions.forEach(function(q, i){
                    var isCorrect = answers[i] === q.answer;
                    if (isCorrect) correct++;
                });

                var total = questions.length;
                var percent = Math.round((correct/total)*100);

                // Generate mock intelligence data based on MCQ performance
                var intelligenceAreas = [
                    'Linguistic', 'Logicalâ€“Mathematical', 'Spatialâ€“Visual', 'Musicalâ€“Rhythmic',
                    'Bodilyâ€“Kinesthetic', 'Interpersonal', 'Intrapersonal', 'Naturalistic'
                ];

                var intelligenceData = {};
                intelligenceAreas.forEach(function(area, index){
                    // Generate varied scores around the main performance
                    var variation = Math.random() * 40 - 20; // -20 to +20 variation
                    var baseScore = Math.max(0, Math.min(100, percent + variation));
                    intelligenceData[area] = Math.round(baseScore);
                });

                assessmentResults = {
                    mode: 'mcq',
                    intelligenceData: intelligenceData,
                    dominantIntelligences: getDominantIntelligences(intelligenceData),
                    overallScore: percent,
                    careerStreams: getCareerStreams(intelligenceData),
                    correctAnswers: correct,
                    totalQuestions: total
                };
            }

            function getDominantIntelligences(data){
                var sorted = Object.entries(data).sort((a,b) => b[1] - a[1]);
                return sorted.slice(0, 2).map(item => item[0]);
            }

            function getCareerStreams(data){
                var streams = [];
                Object.entries(data).forEach(function([intelligence, score]){
                    if (score >= 70){
                        switch(intelligence){
                            case 'Logicalâ€“Mathematical': streams.push('Engineering', 'Computer Science', 'Finance', 'Data Analytics'); break;
                            case 'Interpersonal': streams.push('Management', 'HR', 'Counseling', 'Sales'); break;
                            case 'Linguistic': streams.push('Law', 'Journalism', 'Communication'); break;
                            case 'Spatialâ€“Visual': streams.push('Architecture', 'Design', 'Animation'); break;
                            case 'Musicalâ€“Rhythmic': streams.push('Music', 'Performing Arts'); break;
                            case 'Bodilyâ€“Kinesthetic': streams.push('Sports', 'Physiotherapy', 'Dance'); break;
                            case 'Intrapersonal': streams.push('Psychology', 'Writing', 'Philosophy'); break;
                            case 'Naturalistic': streams.push('Environmental Science', 'Agriculture', 'Wildlife Studies'); break;
                        }
                    }
                });
                return [...new Set(streams)]; // Remove duplicates
            }

