                    { q: 'Which is a JavaScript data type?', options: ['integer', 'boolean', 'decimal', 'character'], answer: 1, explain: 'boolean is a primitive JS type.' },
                    { q: 'Bootstrap primarily helps with?', options: ['APIs', 'UI components and layout', 'File storage', 'DNS'], answer: 1, explain: 'Bootstrap offers UI components and grid layout.' },
                    { q: 'img tags load resources from which attribute?', options: ['src', 'href', 'data', 'load'], answer: 0, explain: 'Images use the src attribute.' }
                ],
                intermediate: [
                    { q: 'Which array method returns a new array with elements that pass a test?', options: ['map', 'reduce', 'filter', 'forEach'], answer: 2, explain: 'filter keeps elements that satisfy the predicate.' },
                    { q: 'In CSS, flex container main axis is controlled by?', options: ['align-items', 'justify-content', 'flex-basis', 'gap'], answer: 1, explain: 'justify-content aligns items along the main axis.' },
                    { q: 'Which HTTP status means Unauthorized?', options: ['200', '301', '401', '500'], answer: 2, explain: '401 indicates missing/invalid authentication.' },
                    { q: 'const in JS means?', options: ['Block-scoped immutable binding', 'Function-scoped mutable', 'Global var', 'Class property'], answer: 0, explain: 'const creates a block-scoped binding.' },
                    { q: 'Which is not a semantic HTML element?', options: ['<section>', '<article>', '<div>', '<header>'], answer: 2, explain: '<div> is generic and not semantic.' }
                ],
                advanced: [
                    { q: 'Big-O of binary search on sorted array?', options: ['O(n)', 'O(log n)', 'O(n log n)', 'O(1)'], answer: 1, explain: 'Binary search halves the space â†’ O(log n).' },
                    { q: 'Which improves Core Web Vitals LCP?', options: ['Defer non-critical CSS', 'Increase DOM size', 'Remove caching', 'Use large images'], answer: 0, explain: 'Less render-blocking improves LCP.' },
                    { q: 'CSR vs SSR: SSR advantage?', options: ['Lower TTFB', 'Better initial SEO', 'More JS required', 'No server needed'], answer: 1, explain: 'SSR improves initial indexability.' },
                    { q: 'Promise.allSettled returns?', options: ['Only fulfilled', 'Only rejected', 'All with status', 'First settled'], answer: 2, explain: 'It returns the status/results for all promises.' },
                    { q: 'Which header enables CORS?', options: ['Content-Type', 'Authorization', 'Access-Control-Allow-Origin', 'ETag'], answer: 2, explain: 'ACAO controls allowed origins.' }
                ]
            };

            // Likert statements (sample). Value map: Mostly Agree=4, Slightly Agree=3, Slightly Disagree=2, Mostly Disagree=1
            var likertOptions = ['Mostly Agree','Slightly Agree','Slightly Disagree','Mostly Disagree'];
            var likertStatements = {
                plus2: {
                    career: [
                        { s: 'I enjoy solving logical puzzles and problems.', dim: 'Logicalâ€“Mathematical' },
                        { s: 'I like explaining ideas using words and examples.', dim: 'Linguistic' },
                        { s: 'I understand peopleâ€™s feelings easily.', dim: 'Interpersonal' },
                        { s: 'I prefer drawing, maps, or diagrams to learn.', dim: 'Spatialâ€“Visual' },
                        { s: 'I reflect on my strengths and weaknesses often.', dim: 'Intrapersonal' },
                        { s: 'I enjoy nature and outdoor environments.', dim: 'Naturalistic' },
                        { s: 'I like rhythm, sound patterns, or music.', dim: 'Musicalâ€“Rhythmic' },
                        { s: 'I learn best by doing and moving.', dim: 'Bodilyâ€“Kinesthetic' }
                    ]
                },
                graduate: {
                    skills: [
                        { s: 'I communicate ideas clearly in teams.', dim: 'Communication' },
                        { s: 'I handle conflicts calmly and constructively.', dim: 'Teamwork' },
                        { s: 'I plan and manage time effectively.', dim: 'Time Management' },
                        { s: 'I adapt quickly to new tools or processes.', dim: 'Adaptability' }
                    ]
                }
            };

            function getQuestions(){
                var list = questionsByLevel[userLevel] || questionsByLevel.beginner;
                return list.slice();
            }

            var questions = [];
            var mode = 'mcq';
 
            function goPrev(){
                if (!questions || !questions.length) return;
                if (currentIndex > 0){
                    currentIndex--;
                    renderQuestion();
                }
            }

            function goNext(){
                if (!questions || !questions.length) return;
                if (currentIndex < questions.length - 1){
                    currentIndex++;
                    renderQuestion();
                }
            }

            function renderQuestion(){
                progressBadge.textContent = 'Question ' + (currentIndex + 1) + '/' + questions.length;
                levelBadge.textContent = 'Level: ' + (userLevel || '-');
                if (progressBar){
                    var pct = Math.round(((currentIndex) / Math.max(1, questions.length)) * 100);
                    progressBar.style.width = pct + '%';
                }
                var q = questions[currentIndex];
                questionText.textContent = mode === 'likert' ? q.s : q.q;
                optionsContainer.innerHTML = '';
                var opts = mode === 'likert' ? likertOptions : q.options;
                opts.forEach(function(opt, idx){
                    var id = 'opt_' + currentIndex + '_' + idx;
                    var item = document.createElement('label');
                    item.className = 'list-group-item d-flex align-items-center';
                    item.setAttribute('for', id);
                    var input = document.createElement('input');
                    input.type = 'radio';
                    input.name = 'q_' + currentIndex;
                    input.id = id;
                    input.value = String(idx);
                    input.className = 'form-check-input me-2';
                    if (answers[currentIndex] === idx) input.checked = true;
                    input.addEventListener('change', function(){
                        answers[currentIndex] = idx;
                        Array.prototype.forEach.call(optionsContainer.querySelectorAll('.list-group-item'), function(el){ el.classList.remove('active'); });
                        item.classList.add('active');
                        updateNavButtons();
                    });
                    var span = document.createElement('span');
                    span.textContent = opt;
                    item.appendChild(input);
                    item.appendChild(span);
                    if (answers[currentIndex] === idx){ item.classList.add('active'); }
                    optionsContainer.appendChild(item);
                });
                updateNavButtons();
            }

            function updateNavButtons(){
                prevBtn.disabled = currentIndex === 0;
                var answered = typeof answers[currentIndex] === 'number';
                // Allow Next to move forward even if unanswered; only disable on last question
                nextBtn.disabled = (currentIndex === questions.length - 1);
                submitBtn.style.display = (currentIndex === questions.length - 1) ? '' : 'none';
                submitBtn.disabled = !answered;
            }

            function showSection(section){
                var introEl = document.getElementById('assessmentIntro');
                var loginPromoEl = document.getElementById('loginPromoContainer');
                var multiResultEl = document.getElementById('multiResultContainer');
                
                if (introEl) introEl.style.display = (section === 'intro') ? 'block' : 'none';
                if (questionContainer) questionContainer.style.display = (section === 'quiz') ? 'block' : 'none';
                if (loginPromoEl) loginPromoEl.style.display = (section === 'loginpromo') ? 'block' : 'none';
                if (multiResultEl) multiResultEl.style.display = (section === 'multiresult') ? 'block' : 'none';
            }

            var chartRef = null;
            function drawChart(labels, data){
                var ctx = document.getElementById('resultChart');
                if (!ctx) return;
                if (chartRef) { chartRef.destroy(); }
