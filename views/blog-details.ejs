                                <div class="ed-blog__details-main">
                                    <div class="ed-blog__details-top">
                                        <!-- Blog Details Cover -->
                                        <div class="ed-blog__details-cover">
                                            <div class="ed-blog__details-cover-img">
                                                <img src="<%= blog.image ? blog.image : '/images/blog/blog-details/b-details-img-1.png' %>"
                                                    alt="<%= blog.title ? blog.title : 'Blog Image' %>" style="object-fit:cover; width:100%; max-height:400px;" />
                                            </div>
                                            <ul class="ed-blog__details-meta">
                                                <li>
                                                    <i class="fi fi-rr-calendar"></i>
                                                    <% if (blog.date) { %>
                                                        <%= new Date(blog.date).toLocaleDateString('en-GB', { day: '2-digit', month: 'long', year: 'numeric' }) %>
                                                    <% } %>
                                                </li>
                                            
                                                <% if (blog.category) { %>
                                                <li>
                                                    <a href="/blogs?category=<%= encodeURIComponent(blog.category) %>"><%= blog.category %></a>
                                                </li>
                                                <% } %>
                                            </ul>
                                        </div>
                                        <h2 class="ed-blog__details-title">
                                            <%= blog.title %>
                                        </h2>
                                        <% if (blog.description) { %>
                                            <div class="ed-blog__details-text">
                                                <%- blog.description %>
                                            </div>
                                        <% } %>
                                        <% if (blog.moredetails) { %>
                                            <br />
                                            <div class="ed-blog__details-text">
                                                <%- blog.moredetails %>
                                            </div>
                                        <% } %>
                                    </div>

                                    <% if (blog.highlightsTitle || (blog.highlightsPoints && blog.highlightsPoints.length > 0)) { %>
                                    <!-- Blog Details Widget -->
                                    <div class="ed-blog__details-widget">
                                        <% if (blog.highlightsTitle) { %>
                                            <h5 class="ed-blog__details-widget-title">
                                                <%= blog.highlightsTitle %>
                                            </h5>
                                        <% } %>
                                        <% if (blog.highlightsPoints && blog.highlightsPoints.length > 0) { %>
                                        <ul class="ed-blog__details-list">
                                            <% blog.highlightsPoints.forEach(function(point) { %>
                                                <li>
                                                    <img src="/images/icons/icon-check-blue.svg" alt="icon-check-blue" />
                                                    <%= point %>
                                                </li>
                                            <% }) %>
                                        </ul>
                                        <% } %>
                                    </div>
                                    <% } %>

                                    <!-- Blog Details Widget Image (optional, if you want to show more images) -->
                                    <% /* 
                                    <div class="ed-blog__details-widget-img">
                                        <img src="/images/blog/blog-details/b-details-img-2.png"
                                            alt="b-details-img-2" />
                                        <img src="/images/blog/blog-details/b-details-img-3.png"
                                            alt="b-details-img-3" />
                                    </div>
                                    */ %>
                                </div>
                            </div>

                            <!-- Sidebar -->
                            <div class="col-lg-6 col-xl-4 col-md-8 col-12">
