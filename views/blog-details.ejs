                                    <!-- Single Sidebar Widget -->
                                    <div class="ed-blog__sidebar-widget">
                                        <h4 class="ed-blog__sidebar-title">Related News</h4>
                                        <div class="ed-blog__latest">
                                            <% if (relatedBlogs && relatedBlogs.length > 0) { %>
                                                <% relatedBlogs.forEach(function(rblog) { %>
                                                    <div class="ed-blog__latest-item">
                                                        <div class="ed-blog__latest-img">
                                                            <img src="<%= rblog.image ? rblog.image : '/images/blog/blog-details/latest-1.png' %>" alt="<%= rblog.title %>" style="object-fit:cover; width:80px; height:80px;" />
                                                        </div>
                                                        <div class="ed-blog__latest-info">
                                                            <a href="/blog-details/<%= encodeURIComponent(rblog.title) %>"><%= rblog.title %></a>
                                                            <span>
                                                                <% if (rblog.date) { %>
                                                                    <%= new Date(rblog.date).toLocaleDateString('en-GB', { day: '2-digit', month: 'long', year: 'numeric' }) %>
                                                                <% } %>
                                                            </span>
                                                        </div>
                                                    </div>
                                                <% }) %>
                                            <% } else { %>
                                                <div class="ed-blog__latest-item">
                                                    <div class="ed-blog__latest-info">
                                                        <span>No related blogs found.</span>
                                                    </div>
                                                </div>
                                            <% } %>
                                        </div>
                                    </div>

                                    <!-- Contact Widget -->
                                    <div class="ed-blog__sidebar-widget">
                                        <h4 class="ed-blog__sidebar-title">Contact Us</h4>
                                        <!-- Sigle Info  -->
                                        <div class="ed-contact__info-item">
                                            <div class="ed-contact__info-icon">
