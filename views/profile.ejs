        <script>
            (function () {
                var editBtn = document.getElementById('editProfileBtn');
                var modalEl = document.getElementById('editProfileModal');
                if (!editBtn || !modalEl) return;
                var modal = new bootstrap.Modal(modalEl);

                function prefill() {
                    var get = function (id) { return document.getElementById(id); };
                    get('fullNameInput').value = (document.getElementById('fullNameValue')?.textContent || '').trim();
                    get('emailInput').value = (document.getElementById('emailValue')?.textContent || '').trim();
                    get('phoneInput').value = (document.getElementById('phoneValue')?.textContent || '').trim();
                    get('studentIdInput').value = (document.getElementById('studentIdValue')?.textContent || '').trim();
                    var dobText = (document.getElementById('dobValue')?.textContent || '').trim();
                    var parsed = Date.parse(dobText);
                    get('dobInput').value = isNaN(parsed) ? '' : new Date(parsed).toISOString().slice(0, 10);
                    get('addressInput').value = (document.getElementById('addressValue')?.textContent || '').trim();
                }

                editBtn.addEventListener('click', function () {
                    prefill();
                    modal.show();
                });

                var saveBtn = document.getElementById('saveProfileBtn');
                saveBtn?.addEventListener('click', function () {
                    var form = document.getElementById('editProfileForm');
                    if (!form.reportValidity()) return;
                    document.getElementById('fullNameValue').textContent = document.getElementById('fullNameInput').value.trim();
                    document.getElementById('emailValue').textContent = document.getElementById('emailInput').value.trim();
                    document.getElementById('phoneValue').textContent = document.getElementById('phoneInput').value.trim();
                    document.getElementById('studentIdValue').textContent = document.getElementById('studentIdInput').value.trim();
                    var dobVal = document.getElementById('dobInput').value;
                    if (dobVal) {
                        var d = new Date(dobVal + 'T00:00:00');
                        var opts = { year: 'numeric', month: 'long', day: 'numeric' };
                        document.getElementById('dobValue').textContent = d.toLocaleDateString(undefined, opts);
                    }
                    document.getElementById('addressValue').textContent = document.getElementById('addressInput').value.trim();
                    modal.hide();
                    var toastEl = document.getElementById('saveToast');
                    if (toastEl) new bootstrap.Toast(toastEl, { delay: 2200 }).show();
                });
            })();
        </script>
