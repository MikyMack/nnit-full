    <style>
        .founder-profile-section {
    background: white;
    color: #333;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    line-height: 1.6;
    padding: 60px 20px;
    min-height: 100vh;
}

.founder-container {
    max-width: 1400px;
    margin: 0 auto;
}

.founder-header {
    text-align: center;
    margin-bottom: 50px;
}

.founder-role {
    font-size: 1.4rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 2px;
    margin-bottom: 15px;
    color: #24519a;
}

.founder-title {
    font-size: 2.8rem;
    font-weight: 700;
    margin-bottom: 20px;
    letter-spacing: 1px;
    background: linear-gradient(135deg, #24519a, #0f1624, #06090f);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}

.founder-name {
    font-size: 1.6rem;
    font-weight: 600;
    margin-top: 15px;
    color: #24519a;
    font-style: italic;
}

.title-decoration {
    height: 4px;
    width: 100px;
    background: linear-gradient(135deg, #24519a, #0f1624, #06090f);
    margin: 0 auto;
    border-radius: 2px;
}

.founder-content {
    background: #f9f9f9;
    border-radius: 12px;
    padding: 40px;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
}

.prologue-section {
    margin-bottom: 40px;
}

.section-divider {
    height: 1px;
    background: rgba(0, 0, 0, 0.1);
    margin: 40px 0;
    width: 100%;
}

.section-title {
    font-size: 1.8rem;
    font-weight: 600;
    margin-bottom: 20px;
    background: linear-gradient(135deg, #24519a, #0f1624, #06090f);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
    position: relative;
    padding-left: 15px;
}

.section-title::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 5px;
    background: linear-gradient(135deg, #24519a, #0f1624, #06090f);
    border-radius: 3px;
}

.section-text {
    font-size: 1.1rem;
    margin-bottom: 25px;
    text-align: justify;
    color: #444;
}

.expertise-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 25px;
    margin: 25px 0;
}

.expertise-item {
    background: white;
    padding: 20px;
    border-radius: 8px;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    border-left: 3px solid #24519a;
    box-shadow: 0 3px 10px rgba(0, 0, 0, 0.08);
}

.expertise-item:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.12);
}

.expertise-subtitle {
    font-size: 1.2rem;
    font-weight: 600;
    margin-bottom: 10px;
    color: #24519a;
}

.expertise-desc {
    font-size: 1rem;
    line-height: 1.6;
    color: #555;
    margin: 0;
}

.cta-section {
    text-align: center;
    margin-top: 50px;
    padding: 30px;
    background: linear-gradient(135deg, #24519a, #0f1624, #06090f);
    border-radius: 8px;
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.cta-text {
    font-size: 1.3rem;
    font-weight: 600;
    margin: 0;
    color: white;
}

/* Responsive Design */
@media (max-width: 992px) {
    .founder-title {
        font-size: 2.4rem;
    }
    
    .founder-content {
        padding: 30px;
    }
    
    .expertise-grid {
        grid-template-columns: 1fr;
    }
}

@media (max-width: 768px) {
    .founder-profile-section {
        padding: 40px 15px;
    }
    
    .founder-title {
        font-size: 2rem;
    }
    
    .founder-content {
        padding: 20px;
    }
    
    .section-title {
        font-size: 1.6rem;
    }
    
    .section-text {
        font-size: 1rem;
    }
    
    .founder-name {
        font-size: 1.3rem;
    }
}

@media (max-width: 480px) {
    .founder-title {
        font-size: 1.8rem;
    }
    
    .section-title {
        font-size: 1.4rem;
    }
    
    .cta-text {
        font-size: 1.1rem;
    }
    
    .founder-role {
        font-size: 1.1rem;
    }
}
